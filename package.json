{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "set NODE_ENV=production && node --import=extensionless/register dist/index.js",
    "dev": "set NODE_ENV=development && tsx watch index.ts",
    "update": "set NODE_ENV=development && tsx scripts/update.ts",
    "update:prod": "set NODE_ENV=production && tsx scripts/update.ts"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "change-case": "^5.4.4",
    "check-disk-space": "^3.4.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.14.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "extensionless": "^1.9.9",
    "firebase-admin": "^13.4.0",
    "node-os-utils": "^1.3.7",
    "random-js": "^2.1.0",
    "systeminformation": "^5.27.5",
    "@types/change-case": "^0.0.30",
    "@types/node": "^24.0.3",
    "@types/node-os-utils": "^1.3.4"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
